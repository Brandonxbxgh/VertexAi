# Solana - Required
SOLANA_PRIVATE_KEY=your_base58_private_key_here
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Jupiter - Required for swap quotes (get free key at https://station.jup.ag/)
JUPITER_API_KEY=your_jupiter_api_key_here

# Optional: Use Helius free tier for better rate limits
# SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api_key=YOUR_KEY

# Arbitrage (Phase 2) - Strict mode defaults
# TRADE_SIZE_LAMPORTS=10000000   # 0.01 SOL per cycle
# MIN_PROFIT_BPS=100             # 1% min profit (strict)
# SAFETY_BUFFER_BPS=20           # +0.2% buffer for execution delay
# MAX_PRICE_IMPACT_PCT=0.5       # Reject if any leg > 0.5% impact
# ARB_SLIPPAGE_BPS=30            # 0.3% slippage (tighter)
# REQUOTE_BEFORE_EXECUTE=true    # Re-check before executing
# POLL_INTERVAL_MS=5000           # Check every 5 seconds

# Supabase - For trade logging (Phase 3+)
# SUPABASE_URL=https://xxx.supabase.co
# SUPABASE_SERVICE_KEY=your_service_role_key

# Telegram - Optional alerts when trade executes (see TELEGRAM_SETUP.md)
# TELEGRAM_BOT_TOKEN=your_bot_token
# TELEGRAM_CHAT_ID=your_chat_id

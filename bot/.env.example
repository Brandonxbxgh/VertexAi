# ===========================================
# Solana (required)
# ===========================================
SOLANA_PRIVATE_KEY=your_base58_private_key
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_KEY

# ===========================================
# Jupiter (required for swaps)
# ===========================================
JUPITER_API_KEY=your_jupiter_api_key

# ===========================================
# Arbitrage (optional - defaults in config.ts)
# ===========================================
# MIN_PROFIT_BPS=25
# SAFETY_BUFFER_BPS=8
# TRADE_SIZE_MIN_SOL=0.01
# TRADE_SIZE_MAX_SOL=0.5
# MAX_PRICE_IMPACT_PCT=0.8
# ARB_SLIPPAGE_BPS=60
# REQUOTE_BEFORE_EXECUTE=true
# POLL_INTERVAL_MS=6000  # 6s keeps under Jupiter 60 req/min; ~1 heartbeat/min

# ===========================================
# Supabase (required for deposits, payouts, logging)
# ===========================================
# SUPABASE_URL=https://xxx.supabase.co
# SUPABASE_SERVICE_KEY=your_service_role_key

# ===========================================
# Telegram (optional - trade alerts)
# ===========================================
# TELEGRAM_BOT_TOKEN=your_bot_token
# TELEGRAM_CHAT_ID=your_chat_id

# ===========================================
# Other (optional)
# ===========================================
# POOL_WALLET=                    # Defaults to bot wallet
# DEPOSIT_POLL_INTERVAL_MS=60000  # Deposit watcher poll (60s default)
